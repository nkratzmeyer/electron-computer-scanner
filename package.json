{
  "name": "wmi-scanner",
  "version": "1.0.0",
  "description": "App to scan WMI info from network PCs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --exec electron . --ignore *.json",
    "build:windows": "electron-builder --win --dir",
    "dist:windows": "electron-builder --win"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Scanner",
    "WMI"
  ],
  "author": "Nathan Kratzmeyer",
  "license": "CC0-1.0",
  "build": {
    "win": {
      "extraResources": [
        {
          "to": "../",
          "filter": [
            "db.json",
            ".env"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "activedirectory2": "^1.3.0",
    "dotenv": "^8.2.0",
    "electron-prompt": "^1.5.1",
    "lowdb": "^1.0.0",
    "node-wmi": "0.0.5",
    "ping": "^0.2.2",
    "wmi-datetime": "^1.0.3"
  }
}
